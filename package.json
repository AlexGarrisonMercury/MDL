{
  "name": "ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "storybook:html": "start-storybook -s ./vendor -p 9005 -c .storybook/html",
    "storybook:vue": "start-storybook -p 9006 -c .storybook/vue",
    "storybook:react": "NODE_PATH=styles:components start-storybook -p 9007 -c .storybook/react",
    "generate-colors-css": "node -e 'require(\"./styles/generate-style-stories\").createCssClassesFromColors()'",
    "deploy-storybook": "storybook-to-ghpages -s build:storybook -o gh-pages",
    "build:storybook": "build-storybook -c .storybook/react -o gh-pages",
    "build-external-ui": "cd packages/external-ui-react && npm run build",
    "build-internal-ui": "cd packages/internal-ui-react && npm run build",
    "build-all": "npm run build-external-ui && npm run build-internal-ui",
    "lint:js": "npx eslint './**/*.js'",
    "test": "npm run lint:js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "eslintConfig": {
    "extends": "@masonite/eslint-config"
  },
  "eslintIgnore": [
    "**/node_modules"
  ],
  "stylelint": {
    "extends": "@masonite/stylelint-config"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@masonite/eslint-config": "0.0.12",
    "@masonite/stylelint-config": "0.0.7",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "babel-preset-react-app": "^6.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.13.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "lerna": "^3.4.3",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.0.0",
    "stylelint-config-recommended": "^2.1.0",
    "svg-inline-loader": "^0.8.0"
  },
  "dependencies": {
    "@dump247/storybook-state": "^1.5.1",
    "@masonite/svg-icons": "^1.0.2",
    "@storybook/addon-actions": "^4.0.7",
    "@storybook/addon-knobs": "^4.0.7",
    "@storybook/addon-options": "^4.0.7",
    "@storybook/addon-storysource": "^4.0.7",
    "@storybook/addon-viewport": "^4.0.7",
    "@storybook/html": "^4.0.7",
    "@storybook/react": "^4.0.7",
    "@storybook/storybook-deployer": "^2.8.1",
    "@storybook/vue": "^4.0.7",
    "classnames": "^2.2.6",
    "react": "^16.8.1",
    "tinycolor2": "^1.4.1"
  }
